environment,
    tabular_data

method,
  bayes_calibration queso
    chain_samples = 500
    dram
chain_diagnostics
output debug


variables,
  uniform_uncertain 1
    upper_bounds  10.5
    lower_bounds 6.31
    descriptors 'Lv'


interface,
        fork
        batch
        analysis_drivers = 'python3 driver.py params.in results.out'
        parameters_file = 'params.in'
        results_file    = 'results.out'
        verbatim


responses,
  calibration_terms = 1    

  field_calibration_terms = 1
  lengths = 1
  num_coordinates_per_field 1

  calibration_data
  variance_type = 'diagonal'
  descriptors = 'CURVE1'
  no_gradients
  no_hessians